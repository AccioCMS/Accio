{
  "name": "acciocms/accio",
  "description": "Accio CMS",
  "keywords": ["cms", "accio", "laravel"],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.0",
    "acciocms/core": "dev-development",
    "chumper/zipper": "1.0.x",
    "doctrine/dbal": "^2.5",
    "spatie/laravel-image-optimizer": "^1.1",
    "fideloper/proxy": "^4.0",
    "htmlmin/htmlmin": "^5.6",
    "intervention/image": "^2.3",
    "laravel/framework": "5.6.*",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^5.2.0",
    "ps/image-optimizer": "^1.2",
    "riverskies/laravel-mobile-detect": "^1.2"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.7",
    "larapack/config-writer": "1.*",
    "laravel/dusk": "^3.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "^7.0",
    "symfony/css-selector": "3.1.*",
    "symfony/dom-crawler": "3.1.*",
    "symfony/thanks": "^1.0",
    "czproject/git-php": "^3.13"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/",
      "Themes\\": "themes/",
      "Plugins\\": "plugins/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/dusk"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "Accio\\App\\Services\\ComposerScripts::postPackageInstall"
    ],
    "post-create-project-cmd": [
      "Accio\\App\\Services\\ComposerScripts::postCreateProject"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
