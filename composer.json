{
    "name": "acciocms/accio",
    "description": "Accio CMS",
    "type": "project",
    "keywords": ["cms", "accio", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "acciocms/nucleus": "dev-dev",
        "chumper/zipper": "1.0.x",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "^4.0",
        "genealabs/laravel-model-caching": "^0.3.3",
        "htmlmin/htmlmin": "^5.6",
        "intervention/image": "^2.3",
        "laravel/framework": "5.7.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.2.0",
        "predis/predis": "^1.1",
        "riverskies/laravel-mobile-detect": "^1.2",
        "spatie/laravel-activitylog": "^2.5",
        "spatie/laravel-backup": "^5.7",
        "spatie/laravel-image-optimizer": "^1.3"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0",
        "laravel/dusk": "^3.0",
        "spatie/db-dumper": "^2.10",
        "larapack/config-writer": "1.*",
        "symfony/css-selector": "3.1.*",
        "symfony/thanks": "^1.0",
        "czproject/git-php": "^3.13",
        "barryvdh/laravel-debugbar": "^3.1"
    },
    "repositories": [],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Themes\\": "themes/",
            "Plugins\\": "plugins/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
